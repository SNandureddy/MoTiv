import Foundation
import UIKit

extension UIFont {
  
  enum MotivFont: String {
    case regular = "Gotham-Bold"
    case light = "Gotham-Light"
    case bold = "Gotham-Black"
    
    func fontWithSize(size: CGFloat) -> UIFont {
      return UIFont(name: rawValue, size: size)!
    }
  }
}
